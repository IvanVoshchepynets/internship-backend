generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Feed {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  title    String
  link     String   @unique
  pubDate  DateTime
  preview  String
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}
